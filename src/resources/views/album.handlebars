<div class="container">    
    <h2>{{this.lbl}}</h2>
    <div class="row">
        {{#each this.results}}
        <div class="col-md-3">
            <div class="thumbnail">
                <a href="{{this.url}}" target="_blank">
                    <img src="{{this.url}}" alt="Lights" style="width:100%">
                </a>
            </div>
        </div>
        {{/each}}
    </div>    
</div>






